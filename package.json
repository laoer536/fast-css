{
  "name": "fast-scss",
  "version": "0.0.3",
  "description": "scss-mixin",
  "main": "dist/index.scss",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "postcss packages/src/**/*.scss --dir packages/dist",
